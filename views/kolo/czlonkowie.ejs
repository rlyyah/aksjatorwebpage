<% include ../partials/header %>
<% include ../partials/dzialalnosc/photolist %>




<div class="container">
	<div class="row">
		<div class="col-md-3"></div>
		<div class="col-md-9 text-center"><h3 class="header-content mt-5"> Członkowie </h3></div>
	</div>
	<div class="row mt-3">
		
		<% include ../partials/leftmenu %>
		
		<div class="col-md-9 main-display">
			
			
			
			
			<div class="row">
        <div class="col-md-6">
			<table class="table">
			    <thead>
			        <tr>
                        <th scope="col" class="text-center">Członkowie</th>
			        </tr>
			    </thead>
			    <tbody>
			        <% members[0].forEach(function(stud){%>
				        <tr>
				        	<td class="text-center">
				        	    <%=stud%>
				        	</td>
				        </tr>
			        <% });%>
			    </tbody>
			</table>
        </div>        
        <div class="col-md-6">
			<table class="table">
			    <thead>
			        <tr>
                        <th scope="col" class="text-center">Absolwenci</th>
			        </tr>
			    </thead>
			    <tbody>
			        <% members[1].forEach(function(grad){%>
				        <tr width="100%">
				        	<td class="text-center">
				        	    <%=grad%>
				        	</td>
				        </tr>
			        <% });%>
			    </tbody>
			</table>
        </div>
        
    </div>
			
		</div>
	</div>
</div>


<% include ../partials/footer %>